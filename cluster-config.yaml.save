apiVersion: eksctl.io/v1alpha5
kind: ClusterConfig

metadata:
  name: microservicios-cluster
  region: us-east-1

vpc:
  id: vpc-05ca7f7xxxxxxxxxx       # el ID exacto de tu VPC
  subnets:
    public:
      us-east-1e:
        id: subnet-00ba9c341cd1c2d08  # la subnet que encontraste

nodeGroups:
  - name: standard-nodes
    instanceType: t3.micro
    desiredCapacity: 2

